{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
    "semi": ["error", "never"],
    "object-curly-newline": ["error", "always"],
    "import/no-extraneous-dependencies": [
      "error",
      {"devDependencies": true}
    ]
  }

}